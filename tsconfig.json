{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "lib": ["ES2022"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node10",
    "skipLibCheck": true,
    "types": [],
    "typeRoots": []
  },
  "include": ["src/**/*.ts"],
  "exclude": ["dist", "node_modules"]
}
